{
  "name": "@swert/feather-icons",
  "version": "0.0.4",
  "description": "simply beautiful open source icons as vue functional components.",
  "author": "Swert <opensource@swert.ru>",
  "license": "MIT",
  "repository": {
    "url": "swert-tech/feather-icons",
    "type": "git"
  },
  "main": "dist/feather-icons.umd.js",
  "module": "dist/feather-icons.es.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/feather-icons.es.js",
      "require": "./dist/feather-icons.umd.js"
    }
  },
  "files": [
    "dist",
    "icons",
    "types"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:icons && vite build",
    "build:icons": "node lib/build && babel src/components -d icons",
    "serve": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.21"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@hcysunyang/babel-plugin-vue-next-jsx": "^0.4.1",
    "eslint-config-rem": "^4.0.0",
    "feather-icons": "^4.28.0",
    "fs-extra": "^8.0.1",
    "pascal-case": "^2.0.1",
    "poi": "^12.4.4",
    "typescript": "^4.4.4",
    "vite": "^2.6.4"
  }
}
