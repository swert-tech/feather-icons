{
  "name": "@swert/feather-icons",
  "version": "0.0.6",
  "description": "simply beautiful open source icons as vue functional components.",
  "author": "Swert <packages@swert.ru>",
  "license": "MIT",
  "repository": {
    "url": "git+https://github.com/swert-tech/feather-icons.git",
    "type": "git"
  },
  "main": "dist/feather-icons.umd.js",
  "module": "dist/feather-icons.es.js",
  "types": "src/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/feather-icons.es.js",
      "require": "./dist/feather-icons.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/swert-tech/feather-icons/issues"
  },
  "homepage": "https://github.com/swert-tech/feather-icons#readme",
  "keywords": [
    "vue",
    "feather-icons",
    "swert"
  ],
  "scripts": {
    "dev": "vite",
    "build": "pnpm build:icons && vite build",
    "build:icons": "pnpm esno build",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "vue": "^3.2.22"
  },
  "devDependencies": {
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "esno": "^0.13.0",
    "feather-icons": "^4.28.0",
    "fs-extra": "^8.1.0",
    "pascal-case": "^2.0.1",
    "typescript": "^4.5.2",
    "vite": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^3"
  }
}
